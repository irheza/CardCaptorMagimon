package magician;

import java.util.ArrayList;

import android.app.Application;
import android.content.Context;
import android.telephony.TelephonyManager;
import magimon.Magimon;

public class Magician extends Application {
	private static Magician magician = null;
	private ArrayList<Magimon> personalMagimon;
	public static String userID;
	int level;
	double experience;
	
	@Override
	public void onCreate(){
		super.onCreate();
		magician = this;
	}
	
	public static Magician getMagician()
	{
		return magician;
	}
	
	
	public Magician(String userID)
	{
		this.userID = userID;
	}
}
