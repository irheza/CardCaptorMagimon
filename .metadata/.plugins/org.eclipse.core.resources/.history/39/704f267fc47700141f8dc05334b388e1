package magimon;

public class Magimon {
	public String id;
	public String name;
	public double attack;
	public double defense;
	public String image;
	
	
	public Magimon(String id)
	{
		this.id=id;
	}
	
	public Magimon(String id, String name, double attack, double defense, String image)
	{
		this.id=id;
		this.name = name;
		this.attack = attack;
		this.defense = defense;
		this.image = image;
	}
}
